<!DOCTYPE html>

<html>
  <head>
    <title>MapMyPath</title>
    <%= csrf_meta_tags %>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBygQhRnDSS9s1hu7jxsQMu3mwIU7Hd2N4&libraries=geometry">
    </script>
    <script src="https://use.fontawesome.com/db1f8543fe.js"></script>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= favicon_link_tag image_path("favicon.ico") %>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
  </head>

  <body class='body'>
    <%= yield %>
  </body>
</html>
